<div class="center">
  <h1>List of Restaurants</h1>

<% @restaurants.each do |rest|  %>
  <p><b>Name:</b> <%= link_to rest.name, restaurant_path(rest) %> <br>
  <b>Category:</b> <%= rest.category %> <br>
  <b>Price Range:</b> <%= rest.price %> <br>
  <b>Capacity:</b> <%= rest.capacity %></p>
<% end %>
